<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" name="viewport" />
    <meta content="ie=edge" http-equiv="x-ua-compatible">
    <title>@ViewData["Title"] - eve.direct</title>
    <environment include="Production">
    <link href="/s.css" rel="stylesheet"/>
    </environment>
    <environment include="Development">
    <link href="~/app.css" rel="stylesheet" />
    <link href="~/dx.common.css" rel="stylesheet" />
    <link href="~/dx.light.css" rel="stylesheet" />
    </environment>
    @* DevExtreme *@
    @* Uncomment to use the HtmlEditor control *@
    @* <script src="https://cdnjs.cloudflare.com/ajax/libs/quill/1.3.6/quill.min.js"></script> *@
    @* Uncomment to use the Gantt control *@
    @*<link href="~/css/devextreme/dx-gantt.css" rel="stylesheet" />*@
    @* Uncomment to use the Diagram control *@
    @*<link href="~/css/devextreme/dx-diagram.css" rel="stylesheet" />*@
</head>
<body>
    @* Виджет процесса загрузки страницы *@
    <div class="page-cover"></div>
    @* Виджет процесса загрузки страницы *@
    @* Контейнер страницы *@
    <div id="page-container" class="page-container page-content-full-height show fade page-header-fixed page-sidebar-fixed">
        @* begin Header *@
        <div id="header" class="header navbar-default">
            @* begin navbar-header *@
            <div class="navbar-header">
                <a href="/" class="navbar-brand">
                    <span class="navbar-logo"></span>
                    <b>Eve</b> Direct
                </a>
                <button type="button" class="navbar-toggle" data-click="sidebar-toggled">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
            </div>
            @* end navbar-header *@
            @* begin header-nav *@
            <ul class="navbar-nav navbar-right">
                <li class="navbar-form">
                    <div id="searchWdt"></div>
                </li>
                @*<li class="navbar-form">
                    <form action="" method="POST" name="search_form">
                        <div class="form-group">
                            <input type="text" class="form-control" placeholder="" />
                            <button type="submit" class="btn btn-search"><i class="fa fa-search"></i></button>
                        </div>
                    </form>
                </li>*@
                @*<li class="dropdown">
                    <a href="#" data-toggle="dropdown" class="dropdown-toggle f-s-14" aria-expanded="false">
                        <i class="fa fa-bell"></i>
                        <span class="label">0</span>
                    </a>
                </li>*@
            </ul>
            @* end header-nav *@
        </div>
        @* end Header *@
        @* Левое меню *@
        <div id="sidebar" class="sidebar">
            @* begin sidebar scrollbar *@
            <div data-scrollbar="true" data-height="100%">
                @* User menu *@
                <ul class="nav">
                    <li class="nav-profile">
                        <a href="#" data-toggle="nav-profile">
                            <div class="cover with-shadow"></div>
                            <div class="image">
                                <img src="/default-username_128.jpg" alt="No face image">
                            </div>
                            <div class="info">
                                <b class="i-caret"></b>
                                <span t="IM0_GUEST"></span>
                                <small t="IM0_Rank" t1="0"></small>
                            </div>
                        </a>
                    </li>
                    <li>
                        <ul class="nav nav-profile closed" style="display: none;">
                            @*<li><a href="#"><i class="fa fa-cogs"></i>Settings</a></li>
                            <li><a href="#"><i class="fa fa-id-badge"></i>SSO tokens</a></li>*@
                            <li>
                                <a href="#" t="IM0_Login">
                                    <i class="i-key"></i>
                                </a>
                            </li>
                            <li>
                                <a href="#" t="IM0_Register">
                                    <i class="i-sign-in"></i>
                                </a>
                            </li>
                        </ul>
                    </li>
                </ul>
                @* end User menu*@
                @* begin sidebar nav *@
                <ul class="nav">
                    <li class="nav-search">
                        <input type="text" class="form-control" placeholder="" data-sidebar-search="true">
                    </li>
                    <li class="nav-header"></li>
                    <li class="has-sub">
                        <a href="#">
                            <b class="i-caret"></b>
                            <i class="i-tags"></i>
                            <span t="IM0_MARKET"></span>
                        </a>
                        <ul class="sub-menu">
                            <li><a href="/@Routes.MarketOrders" t="IM0_MARKET_ORDERS"></a></li>
                            <li><a href="/@Routes.MarketContracts" t="IM0_MARKET_CONTRACTS"></a></li>
                        </ul>
                    </li>
                    <li class="has-sub">
                        <a href="#">
                            <b class="i-caret"></b>
                            <i class="i-starfighter"></i>
                            <span t="IM0_WARFACE"></span>
                        </a>
                        <ul class="sub-menu">
                            <li><a href="/@Routes.WarfaceKills" t="IM0_WARFACE_KILLS"></a></li>
                            <li><a href="/@Routes.WarfaceWars" t="IM0_WARFACE_WARS"></a></li>
                        </ul>
                    </li>
                    <li class="has-sub" id="ch">
                        <a href="#">
                            <b class="i-caret"></b>
                            <i class="i-user"></i>
                            <span t="IM0_CHARACTERS"></span>
                        </a>
                        <ul class="sub-menu">
                            <li id="1"><a href="/@Routes.CharacterMigrations" t="IM0_CHARACTERS_MIGRATIONS"></a></li>
                            <li id="2"><a href="/@Routes.CharacterNewBorns" t="IM0_CHARACTERS_NEWBORNS"></a></li>
                        </ul>
                    </li>
                    <li class="has-sub">
                        <a href="#">
                            <b class="i-caret"></b>
                            <i class="i-user-friends"></i>
                            <span t="IM0_CORPORATIONS"></span>
                        </a>
                        <ul class="sub-menu">
                            <li id="3"><a href="/@Routes.CorporationMigrations" t="IM0_CORPORATIONS_MIGRATIONS"></a></li>
                            <li id="4"><a href="/@Routes.CorporationNew" t="IM0_CORPORATIONS_NEW"></a></li>
                        </ul>
                    </li>
                    <li class="has-sub">
                        <a href="#">
                            <b class="i-caret"></b>
                            <i class="i-shield"></i>
                            <span t="IM0_ALLIANCES"></span>
                        </a>
                        <ul class="sub-menu">
                            <li id="4"><a href="/@Routes.AlliancesNew" t="IM0_ALLIANCES_NEW"></a></li>
                        </ul>
                    </li>
                    <li class="has-sub">
                        <a href="#">
                            <b class="i-caret"></b>
                            <i class="i-sitemap"></i>
                            <span t="IM0_UNIVERSE"></span>
                        </a>
                        <ul class="sub-menu">
                            <li><a href="/@Routes.UniverseMap" t="IM0_UNIVERSE_MAP"></a></li>
                            <li><a href="/@Routes.UniverseTypes" t="IM0_UNIVERSE_TYPES"></a></li>
                        </ul>
                    </li>
                    @* begin sidebar minify button *@
                    <li>
                        <a href="#" class="sidebar-minify-btn" data-click="sidebar-minify">
                            <i class="i-angle-double-left"></i>
                        </a>
                    </li>
                    @* end sidebar minify button *@
                </ul>
                @* end sidebar nav *@
            </div>
            @* end sidebar scrollbar *@
        </div>
        <div class="sidebar-bg"></div>
        @* Левое меню *@
        @* Контент страницы *@
        <div id="content" class="content">
            <div class="row">
                <div class="col-12">
                    @* begin breadcrumb *@
                    <ol class="breadcrumb pull-right" id="brd"></ol>
                    @* end breadcrumb *@
                    @* begin page-header *@
                    <h2 class="page-header" id="ph"></h2>
                    @* end page-header *@
                </div>
            </div>
            @RenderBody()
        </div>
        <div class="footer">
            <p class="p-t-10 p-b-10 pull-left">© "Eve Direct" LLC, 2020</p>
            @* Переключение локализации *@
            <div id="select-language" class="pull-right"></div>
        </div>
        @* Контент страницы *@
        @* Кнопка "Пролистать наверх" *@
        <a href="#" class="btn btn-icon btn-circle btn-success btn-scroll-to-top fade" data-click="scroll-top"><i class="i-angle-up"></i></a>
        @* Кнопка "Пролистать наверх" *@
        @* Индикатор подгрузки *@
        <div id="loadpanel"></div>
    </div>
    @* Контейнер страницы *@
    @* JS *@
    @*<script src="../assets/js/theme/default.min.js"></script>*@
    @* Uncomment to use the Gantt control *@
    @*<script src="~/js/devextreme/dx-gantt.js"></script>*@
    @* Uncomment to use the Diagram control *@
    @*<script src="~/js/devextreme/dx-diagram.js"></script>*@
    @* Uncomment to enable client-side export *@
    @*<script src="~/js/devextreme/jszip.js"></script>*@
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jQuery-slimScroll/1.3.8/jquery.slimscroll.min.js"></script>
    @* DevExtreme *@
    <script src="/dx.all.js"></script>
    @* Кастомные скрипты *@
    <script src="~/js/app.js" type="module"></script>
    @* Uncomment to provide geo-data for the VectorMap control *@
    @*<script src="~/js/devextreme/vectormap-data/world.js"></script>*@
    @*<script src="~/dx.aspnet.mvc.js"></script>
        <script src="~/dx.aspnet.data.js"></script>*@
    @{
        RenderSection("Scripts", required: false);
    }
</body>
</html>
